<h1>New Job</h1>

<%= form_for(@job) do |f| %>
    <%= f.label :title %>
    <%= f.text_field :title %>
    <br>
    <br>
    <%= f.label :company %>
    <%= f.text_field :company %>
    <br>
    <br>
    <%= f.label :content %>
    <%= f.text_field :content %>
    <br>
    <br>
    <%= f.hidden_field :user_id, value: current_user.id %>
    <br>
    <%= f.submit "Submit" %>

<% end %>